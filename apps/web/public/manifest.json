  "file_handlers": [
    {
      "action": "/handle-qr",
      "accept": {
        "image/*": [".png", ".jpg", ".jpeg", ".gif", ".webp"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "capture_links": "existing-client-navigate",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "permissions": [
    "geolocation",
    "notifications",
    "microphone",
    "camera",
    "payment-handler"
  ],
  "features": [
    "CrossOriginEmbedderPolicy",
    "CrossOriginOpenerPolicy",
    "PaymentRequest",
    "WebShare",
    "BackgroundSync",
    "VoiceRecognition",
    "Geolocation",
    "PushMessaging"
  ],
  "display_override": [
    "window-controls-overlay",
    "minimal-ui",
    "standalone",
    "fullscreen"
  ],
  "user_preferences": {
    "color_scheme_dark": {
      "theme_color": "#1a1a1a",
      "background_color": "#000000"
    }
  },
  "handle_links": "preferred",
  "offline_enabled": true,
  "offline_fallback": "/offline.html",
  "serviceworker": {
    "src": "/service-worker.js",
    "scope": "/",
    "type": "module",
    "update_via_cache": "none"
  },
  "gcm_sender_id": "103953800507",
  "fcm_options": {
    "analytics_label": "eatech-pwa-analytics"
  },
  "background_sync": [
    "order-sync",
    "analytics-sync",
    "voice-command-sync"
  ],
  "web_push": {
    "vapid_public_key": "BLU2Q2v2DhJrCqEQNZ6s_jnKjzQl7P8DYlvf6KH8a5R4J7sP_8C5M2b_B3J7e_9f",
    "registration_endpoint": "https://api.eatech.ch/push/register"
  },
  "custom": {
    "eatech_config": {
      "api_base_url": "https://api.eatech.ch",
      "websocket_url": "wss://ws.eatech.ch",
      "cdn_base_url": "https://cdn.eatech.ch",
      "voice_wake_word": "Hey EATECH",
      "supported_languages": ["de-CH", "fr-CH", "it-CH", "en-CH"],
      "supported_payment_methods": ["card", "twint", "cash", "postfinance"],
      "offline_capabilities": [
        "menu_browsing",
        "cart_management",
        "order_creation",
        "voice_commands",
        "customer_data",
        "analytics_tracking"
      ],
      "swiss_features": {
        "qr_invoice_support": true,
        "twint_integration": true,
        "postfinance_support": true,
        "swiss_tax_calculation": true,
        "multi_language_canton_support": true,
        "vat_number_validation": true
      },
      "performance_targets": {
        "first_contentful_paint": 1500,
        "largest_contentful_paint": 2500,
        "first_input_delay": 100,
        "cumulative_layout_shift": 0.1,
        "offline_first_load": 3000
      },
      "accessibility": {
        "high_contrast_support": true,
        "voice_navigation": true,
        "screen_reader_optimized": true,
        "dyslexia_friendly_fonts": true,
        "cognitive_assistance": true
      }
    }
  },
  "workbox": {
    "runtimeCaching": [
      {
        "urlPattern": "https://api.eatech.ch/.*",
        "handler": "NetworkFirst",
        "options": {
          "cacheName": "api-cache",
          "networkTimeoutSeconds": 3,
          "expiration": {
            "maxEntries": 100,
            "maxAgeSeconds": 300
          }
        }
      },
      {
        "urlPattern": "https://cdn.eatech.ch/.*",
        "handler": "CacheFirst",
        "options": {
          "cacheName": "cdn-cache",
          "expiration": {
            "maxEntries": 200,
            "maxAgeSeconds": 2592000
          }
        }
      }
    ],
    "skipWaiting": true,
    "clientsClaim": true
  },
  "contact_info": {
    "support_email": "support@eatech.ch",
    "support_phone": "+41 44 123 45 67",
    "website": "https://eatech.ch",
    "privacy_policy": "https://eatech.ch/privacy",
    "terms_of_service": "https://eatech.ch/terms"
  },
  "monetization": "$ilp.uphold.com/WMbkRyiNgM8W",
  "payment": [
    {
      "supported_methods": ["https://eatech.ch/pay"],
      "data": {
