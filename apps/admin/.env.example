# EATECH Admin Dashboard Environment Variables

# App Configuration
VITE_APP_NAME=EATECH Admin
VITE_APP_VERSION=3.0.0
VITE_APP_URL=http://localhost:3001
VITE_API_URL=http://localhost:5001

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Features
VITE_FEATURE_ANALYTICS=true
VITE_FEATURE_AI_PRICING=true
VITE_FEATURE_VOICE_ORDERS=true
VITE_FEATURE_MULTI_LOCATION=true
VITE_FEATURE_INVENTORY=true
VITE_FEATURE_LOYALTY=true
VITE_FEATURE_EVENTS=true

# Analytics
VITE_PLAUSIBLE_DOMAIN=localhost
VITE_PLAUSIBLE_API_HOST=https://plausible.io
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# Maps
VITE_MAPBOX_ACCESS_TOKEN=your-mapbox-token

# AI Services
VITE_OPENAI_API_KEY=your-openai-key

# Development
VITE_DEBUG_MODE=false
VITE_MOCK_DATA=false

# Multi-tenant
VITE_TENANT_MODE=single
VITE_DEFAULT_TENANT_ID=default
