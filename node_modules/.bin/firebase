#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Benedikt Thomma/Documents/Eatech/node_modules/firebase-tools/lib/bin/node_modules:/mnt/c/Users/Benedikt Thomma/Documents/Eatech/node_modules/firebase-tools/lib/node_modules:/mnt/c/Users/Benedikt Thomma/Documents/Eatech/node_modules/firebase-tools/node_modules:/mnt/c/Users/Benedikt Thomma/Documents/Eatech/node_modules:/mnt/c/Users/Benedikt Thomma/Documents/node_modules:/mnt/c/Users/Benedikt Thomma/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Benedikt Thomma/Documents/Eatech/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Benedikt Thomma/Documents/Eatech/node_modules/firebase-tools/lib/bin/node_modules:/mnt/c/Users/Benedikt Thomma/Documents/Eatech/node_modules/firebase-tools/lib/node_modules:/mnt/c/Users/Benedikt Thomma/Documents/Eatech/node_modules/firebase-tools/node_modules:/mnt/c/Users/Benedikt Thomma/Documents/Eatech/node_modules:/mnt/c/Users/Benedikt Thomma/Documents/node_modules:/mnt/c/Users/Benedikt Thomma/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Benedikt Thomma/Documents/Eatech/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../firebase-tools/lib/bin/firebase.js" "$@"
else
  exec node  "$basedir/../firebase-tools/lib/bin/firebase.js" "$@"
fi
